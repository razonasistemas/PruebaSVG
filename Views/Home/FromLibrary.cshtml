<script>
    // Elimina la linea negra tras 3 segundos. Cambia el texto tambien
    setTimeout(function () {
        var lineaNegra = document.getElementById("line");
        lineaNegra.remove();
        var texto = document.getElementById("texto");
        texto.textContent = "click ya!";
    }, 3000);
</script>

<script>
    // Este codigo busca el elemento con id "clickableRect" y le añade dinamicamente un codigo al evento "onclick"
    element = document.getElementById("clickableRect");
   
    element.onclick = function () {
        alert("Conectando al puerto XXX y ejecutando YYY");
    }
</script>